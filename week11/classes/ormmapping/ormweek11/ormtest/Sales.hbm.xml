<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ormweek11.ormtest.Sales" table="Sales" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="product" column="Product" type="string" length="255" not-null="false" lazy="false"/>
		<property name="date" column="`Date`" type="string" length="255" not-null="false" lazy="false"/>
		<property name="quantity" column="Quantity" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
