<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="ormweek11.ormtest.Employee" table="Employee" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="name" column="Name" type="string" length="255" not-null="false" lazy="false"/>
		<property name="address" column="Address" type="string" length="255" not-null="false" lazy="false"/>
		<property name="mobile" column="Mobile" type="string" length="255" not-null="false" lazy="false"/>
		<property name="PPS" column="PPS" type="string" length="255" not-null="false" lazy="false"/>
		<property name="year" column="Year" type="string" length="255" not-null="false" lazy="false"/>
		<property name="department" column="Department" type="string" length="255" not-null="false" lazy="false"/>
		<property name="salary" column="Salary" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
